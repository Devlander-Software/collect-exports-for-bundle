{
  "name": "@devlander/collect-exports-for-bundle",
  "version": "1.0.33",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/cjs/index.d.ts",
  "type": "commonjs",
  "files": [
    "dist"
  ],
  "description": "Collects all exports from a package.json's main file and writes them to a file.",
  "author": {
    "name": "Landon W. Johnson",
    "url": "https://landonjohnson.dev"
  },
  "scripts": {
    "docs": "typedoc --out ./docs ./src",
    "test": "jest --coverage",
    "test:example": "cd example && yarn install && cd .. &&  node example/bootstrap.cjs",
    "test:snaps": "jest --updateSnapshot",
    "patch": "yarn version --new-version patch",
    "lint": "eslint .",
    "format": "eslint './src/**/*.ts' --fix && prettier --write './src/**/*.ts'",
    "typecheck": "tsc --project tsconfig.json --noEmit --emitDeclarationOnly false",
    "buildESM": "tsc",
    "buildCJS": "rollup -c",
    "timeout": "delay 5",
    "build": "yarn run docs && npx rimraf ./dist && yarn run timeout && yarn run format && yarn run typecheck && yarn run buildESM && yarn run buildCJS && yarn run test:example &&  yarn run patch"
  },
  "license": "MIT",
  "dependencies": {
    "cliui": "^8.0.1",
    "escalade": "^3.1.1",
    "jest": "^29.7.0",
    "picocolors": "^1.0.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.22.17",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-transform-runtime": "^7.22.15",
    "@babel/preset-env": "^7.22.15",
    "@babel/preset-typescript": "^7.22.15",
    "@rollup/plugin-auto-install": "^3.0.5",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.5",
    "@swc/core": "^1.3.94",
    "@types/cli-progress": "^3.11.4",
    "@types/jest": "^29.5.6",
    "@types/node": "^20.8.7",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.8.0",
    "delay-cli": "2.0.0",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jest": "^27.4.2",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
    "prettier": "^3.0.3",
    "rollup": "^3.29.1",
    "rollup-plugin-auto-external": "^2.0.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-dts": "^6.0.2",
    "rollup-plugin-generate-git-version": "^1.0.0",
    "rollup-plugin-generate-package-json": "^3.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-polyfill-node": "^0.12.0",
    "rollup-plugin-string": "^3.0.0",
    "rollup-swc-preserve-directives": "^0.5.0",
    "ts-node": "^10.9.1",
    "typedoc": "^0.25.2"
  },
  "repository": {
    "type": "git",
    "url": "https://gist.github.com/landonwjohnson/2ca297f86cf9e25ae2fcc01752f80908"
  }
}
